// mapserver template
[resultset layer=DEFAULT]
{
  "type": "FeatureCollection",
  "features": [
    [feature trimlast=","]
    {
      "type": "Feature",
      "geometry": {
        "type": "Point",
        "coordinates": [[lon], [lat]]
      },
      "properties": {
        "species": "[species]"
      }
    },
    [/feature]
  ]
}
[/resultset]
