// MapServer GeoJSON Template
[resultset layer=DEFAULT]
{
 "type": "FeatureCollection",
 "crs": {
  "type": "EPSG",
  "properties": {
   "code": 4326,
   "coordinate_order": [1,0]
  }
 },
 "features": [
 [feature trimlast=","] {
   "type": "Feature",
   "id": "[UNIQUE_KEY]",
   "geometry": {
    "type": "Point",
    "coordinates":
     [[Longitude], [Latitude]]
   },
   "properties": {
   }
  },
 [/feature]
 ]
}
[/resultset]
